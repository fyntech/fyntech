---
layout: none
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:http://www.fyntech.dk/
METHOD:PUBLISH
NAME:{{ site.title }}
X-WR-CALNAME:{{ site.title }}
DESCRIPTION:{{ site.description }}
X-WR-CALDESC:{{ site.description }}
TIMEZONE-ID:{{ site.timezone }}
X-WR-TIMEZONE:{{ site.timezone }}
REFRESH-INTERVAL;VALUE=DURATION:PT24H
X-PUBLISHED-TTL:PT24H{%
	assign current_date = site.time | date: "%s" %}{%
	for event in site.events %}{%
	if event.dateEnd %}{%
	 assign expire_date = event.dateEnd | date: "%s" %}{%
	else %}{%
	 assign expire_date = event.dateStart | date: "%s" %}{%
	endif %}{%
	if expire_date < current_date %}{%
    continue %}{%
  endif %}{%
	assign location = event.location | replace: ",", "\," %}
BEGIN:VEVENT
UID:{{ event.organizer | append: event.title | md5 }}-{{ event.dateStart | date: "%Y%m%d" }}@fyntech.dk
ORGANIZER;CN="{{ event.organizer }}":MAILTO:
LOCATION:{{ location | replace: "<br>", "\n" }}
SUMMARY:{{ event.title }}
DESCRIPTION:{{ event.excerpt | replace: "<br />", "\\n" | strip_html | strip_newlines }}
CLASS:PUBLIC
DTSTART:{{ event.dateStart | toUTC | date: "%Y%m%dT%H%M%SZ" }}{%
	if event.dateEnd %}
DTEND:{{ event.dateEnd | toUTC | date: "%Y%m%dT%H%M%SZ" }}{%
	endif %}
DTSTAMP:{{ event.dateStart | date: "%Y%m%d" }}T170000Z
END:VEVENT{%
	endfor %}
END:VCALENDAR
