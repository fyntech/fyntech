---
layout: none
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:http://www.fyntech.dk/
METHOD:PUBLISH
NAME:{{ site.title }}
X-WR-CALNAME:{{ site.title }}
{{ site.description | prepend: "DESCRIPTION:" | split_string }}
{{ site.description | prepend: "X-WR-CALDESC:" |Â split_string }}
TIMEZONE-ID:{{ site.timezone }}
X-WR-TIMEZONE:{{ site.timezone }}
REFRESH-INTERVAL;VALUE=DURATION:PT24H
X-PUBLISHED-TTL:PT24H{%
	assign current_date = site.time | date: "%s" %}{%
	for event in site.events %}{%
	if event.dateEnd %}{%
	 assign expire_date = event.dateEnd | date: "%s" %}{%
	else %}{%
	 assign expire_date = event.dateStart | date: "%s" %}{%
	endif %}{%
	if expire_date < current_date %}{%
    continue %}{%
  endif %}{%
	assign location = event.location | replace: ",", "\," %}{%
  assign link = event.link | prepend: "\n\nSign up / more info: " %}
BEGIN:VEVENT
UID:{{ event.organizer | append: event.title | md5 }}-{{ event.dateStart | date: "%Y%m%d" }}@fyntech.dk
ORGANIZER;CN="{{ event.organizer }}":MAILTO:
{{ location | prepend: "LOCATION:" | replace: "<br>", "\n" | split_string }}
SUMMARY:{{ event.title }}
{{ event.excerpt | prepend: "DESCRIPTION:" | append: link | replace: "<br />", "\\n" | strip_html | strip_newlines | split_string }}
CLASS:PUBLIC
DTSTART:{{ event.dateStart | date_to_utc | date: "%Y%m%dT%H%M%SZ" }}{%
	if event.dateEnd %}
DTEND:{{ event.dateEnd | date_to_utc | date: "%Y%m%dT%H%M%SZ" }}{%
	endif %}
DTSTAMP:{{ event.dateStart | date_to_utc | date: "%Y%m%dT%H%M%SZ" }}
LAST-MODIFIED:{{ site.time | date_to_utc | date: "%Y%m%dT%H%M%SZ" }}
END:VEVENT{%
	endfor %}
END:VCALENDAR